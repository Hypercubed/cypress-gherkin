{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES2017", "dom"],
    "strict": true,
    "types": ["cypress"],
    "noEmit": true,
    "paths": {
      "cypress-gherkin": [
        "src/index"
      ],
      "cypress-gherkin/*": [
        "src/*"
      ]
    }
  },
  "include": ["src/*.ts"]
}